//Q28: Write a program to print the product of even numbers from 1 to n.
#include<stdio.h>
int main(){
  int n,pro=1,even;
  printf("enter an even number till which you want the product\n");
  scanf("%d",&n);
  for(int i=1;2*i<=n;i++){
    even= 2*i;
    pro *= even;
  }
  printf("%d(",pro);
  for(int i=1;2*i<=n;i++){
    even = 2*i;
    if(i==1){
      printf("%d",even);
    }
    else{
      printf("*%d",even);
    }
  }
  printf(")");
  return 0;
}
  
