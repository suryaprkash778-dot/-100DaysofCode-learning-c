//Q27: Write a program to print the sum of the first n odd numbers.

#include<stdio.h>
int main()
{
   int n,odd_num,odd_sum = 0;
   printf("enter a number n ");
   scanf("%d",&n);
   
   for(int i=1;i<=n;i++){
    odd_num = 2*i-1;
    odd_sum += odd_num;
   }
   printf("%d\n",odd_sum);

    return 0;

}

