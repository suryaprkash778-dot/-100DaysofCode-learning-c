//Q57: Find the sum of array elements.
#include <stdio.h>
int main(){
    int arr[100],size,sum=0;
    printf("enter size of array\n");
    scanf("%d",&size);
    printf("enter elements in array\n");
    for(int i=0;i<size;i++){
        scanf("%d",(arr + i));
        sum+=arr[i];
    }
    printf("%d",sum);
    return 0;
}
