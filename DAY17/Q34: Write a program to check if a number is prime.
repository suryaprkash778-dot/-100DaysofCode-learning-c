//Q34: Write a program to check if a number is prime.
#include <stdio.h>
#include<math.h>
void main(){
    int n;
    printf("enter a number\n");
    scanf("%d",&n);
    int isprime=1;
    for(int i=2;i<=sqrt(n);i++){
        if(n%i==0){
            isprime=0;
            break;
            }
        }
        if(isprime){
            printf("Prime\n");
        }
        else{
            printf("Not Prime\n");
        }
        }
