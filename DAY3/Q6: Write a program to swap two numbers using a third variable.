//Q6: Write a program to swap two numbers using a third variable.

#include<stdio.h>
int main()
{
    
    int x,y,z;
    printf("enter first digit\n");
    scanf("%d",&x);
    printf("enter second digit\n");
    scanf("%d",&y);
    printf("before swapping numbers are x=%d and y=%d\n",x,y);
    z = x;
    x=y;
    y=z;
    printf("after swapping numbers are x=%d and y=%d\n",x,y);

    return 0;
}
