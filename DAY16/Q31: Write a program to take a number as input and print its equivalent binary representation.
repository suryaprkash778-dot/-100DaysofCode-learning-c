//Q31: Write a program to take a number as input and print its equivalent binary representation.
#include<stdio.h>
void main(){
  int n;
  printf("enter a number\n");
  scanf("%d",&n);
  int quotient = n;
  int binary = 0;
  int place =1;
  while(quotient>0){
    int rem = quotient%2;
    binary += rem * place;
    place *=10;
    quotient/=2;
  }
  printf("%d",binary);
}
  

   
