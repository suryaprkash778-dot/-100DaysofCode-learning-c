//Q7: Write a program to swap two numbers without using a third variable.

#include<stdio.h>
int main()
{
    
    int x,y,z;
    printf("enter first digit\n");
    scanf("%d",&x);
    printf("enter second digit\n");
    scanf("%d",&y);
    printf("your two digit number is: %d%d\n",x,y);
    
    printf("your two digit number after swaping digits is: %d%d\n",y,x);
    

    return 0;
}
