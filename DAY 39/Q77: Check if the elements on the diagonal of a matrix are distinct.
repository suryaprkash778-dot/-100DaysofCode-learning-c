//Q77: Check if the elements on the diagonal of a matrix are distinct.
#include <stdio.h>
int main() {
    int r, c;
    printf("enter rows and columns of matrix\n");
    scanf("%d %d", &r, &c);
    printf("\n");
    int mat[r][c];
    for (int i = 0; i < r; i++)
        for (int j = 0; j < c; j++)
            scanf("%d", &mat[i][j]);

    int isDistinct = 1;
    for (int i = 0; i < r && i < c; i++) {
        for (int j = i + 1; j < r && j < c; j++) {
            if (mat[i][i] == mat[j][j]) {
                isDistinct = 0;
                break;
            }
        }
        if (!isDistinct) break;
    }

    printf("%s\n", isDistinct ? "True" : "False");
    return 0;
}

