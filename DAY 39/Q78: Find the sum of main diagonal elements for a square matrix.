//Q78: Find the sum of main diagonal elements for a square matrix.
#include <stdio.h>
int main() {
    int n, m;
    printf("enter rows and columns of matrix\n");
    scanf("%d %d", &n, &m);
    printf("\n");

    if (n != m) {
        printf("Not a square matrix\n");
        return 1;
    }

    int mat[n][n];
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &mat[i][j]);

    int sum = 0;
    for (int i = 0; i < n; i++)
        sum += mat[i][i];

    printf("%d\n", sum);
    return 0;
}

