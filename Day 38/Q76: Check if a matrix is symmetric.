//Q76: Check if a matrix is symmetric.
#include <stdio.h>
int main() {
    int r, c;
    printf("enter rows and columns for matrix\n");
    scanf("%d %d", &r, &c);
    printf("\n");
    int mat[r][c];
    for (int i = 0; i < r; i++)
        for (int j = 0; j < c; j++)
            scanf("%d", &mat[i][j]);

    if (r != c) {
        printf("False\n");
        return 0;
    }

    int isSymmetric = 1;
    for (int i = 0; i < r && isSymmetric; i++) {
        for (int j = 0; j < c; j++) {
            if (mat[i][j] != mat[j][i]) {
                isSymmetric = 0;
                break;
            }
        }
    }
    printf("%s\n", isSymmetric ? "True" : "False");
    return 0;
}

